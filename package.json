{
  "name": "oam-browser",
  "version": "2.0.0",
  "catalog_version": "0.6.2",
  "description": "Open Aerial Map Browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/hotosm/oam-browser"
  },
  "author": {
    "name": "Humanitarian Open Street Map Team",
    "url": "https://hotosm.org"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/hotosm/oam-browser/issues"
  },
  "homepage": "http://openaerialmap.org",
  "scripts": {
    "postinstall": "[ -f src/config/local.js ] || echo 'module.exports = {};' > src/config/local.js",
    "build-css": "./contrib/build-css.sh",
    "watch-css": "yarn build-css && yarn build-css -- --watch --recursive",
    "start-js": "NODE_PATH=src react-scripts start",
    "start": "run-p watch-css start-js",
    "build": "yarn build-css && react-scripts build",
    "lint": "eslint .",
    "test": "NODE_ENV=test NODE_PATH=src mocha --opts test/specs/mocha.opts test/specs"
  },
  "devDependencies": {
    "babel-preset-react-app": "^3.0.2",
    "chai": "^3.5.0",
    "chai-webdriverio": "^0.3.0",
    "enzyme": "^2.8.2",
    "eslint": "^4.5.0",
    "eslint-plugin-prettier": "^2.2.0",
    "jeet": "^7.1.0",
    "mocha": "^3.2.0",
    "node-bourbon": "^4.2.8",
    "node-neat": "^1.7.2",
    "node-sass-chokidar": "^0.0.3",
    "prettier": "^1.5.3",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "wdio-browserstack-service": "^0.1.11",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.7.1",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {
    "clipboard": "^1.5.10",
    "form-serialize": "^0.7.1",
    "joi": "git://github.com/hapijs/joi.git#1b3de5c",
    "jquery": "^2.1.4",
    "lodash": "^3.8.0",
    "lodash.defaultsdeep": "^4.3.5",
    "mapbox.js": "^2.1.9",
    "moment": "^2.10.6",
    "nets": "^3.1.0",
    "pretty-bytes": "^2.0.1",
    "rbush": "^1.4.1",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.1.0",
    "react-autocomplete": "^1.5.0",
    "react-dom": "^15.6.1",
    "react-keybinding": "git+https://github.com/gregeinfrank/react-keybinding.git#d733a47",
    "react-router": "^2.4.0",
    "react-scripts": "1.0.11",
    "react-tether": "^0.5.7",
    "react-validation-mixin": "^4.2.0",
    "react-widgets": "^3.4.6",
    "reflux": "^0.4.1",
    "tilebelt": "^1.0.1",
    "titlecase": "^1.0.2",
    "turf-centroid": "^1.1.2",
    "turf-extent": "^1.0.4",
    "turf-inside": "^1.1.4",
    "turf-overlaps": "^1.0.3",
    "wellknown": "^0.5.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
